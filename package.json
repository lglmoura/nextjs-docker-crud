{
  "name": "nextjs-docker-crud",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npm install --force && npm run migrate:dev:reset",
    "dev": "dotenv  --  next dev",
    "build": "next build",
    "prestart": "npm install --force && npm run migrate:dev:reset",
    "start": "next start",
    "start:prod": "node dist/main",
    "dev:docker:down": "docker-compose --env-file ./.env -f docker-compose.yml  down",
    "dev:docker:up": "docker-compose --env-file ./.env -f docker-compose.yml  up -d --build",
    "dev:docker:start": "npm run dev:docker:down && npm run dev:docker:up   && sleep 1",
    "prestartmigr:dev": "npm run dev:docker:start && dotenv  -- prisma migrate reset --force",
    "seed:dev": "dotenv  -- prisma db seed ",
    "prestart:dev": "npm run dev:docker:start",
    "start:dev": "dotenv -- next dev",
    "migrate:dev": "dotenv  -- npx prisma migrate dev",
    "migrate:dev:create": "dotenv  -- npx prisma migrate dev --create-only",
    "migrate:dev:reset": "dotenv  -- npx prisma migrate reset --force",
    "migrate:dev:resolve": "dotenv  -- npx prisma migrate resolve",
    "prisma:dev:studio": "dotenv  -- npx prisma studio",
    "prisma:dev:generate": "dotenv  -- npx prisma generate",
    "start:docker": "dotenv  -- next dev",
    "migrate:docker": "dotenv  -- npx prisma migrate dev",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "daisyui": "^3.1.6",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "8.43.0",
    "eslint-config-next": "^12.0.4",
    "next": "13.4.7",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "prisma": "^5.17.0"
  }
}
